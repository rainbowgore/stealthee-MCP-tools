{
  "name": "web_search",
  "description": "Search the web for information about stealth product launches",
  "inputSchema": {
    "type": "object",
    "properties": {
      "query": {
        "type": "string",
        "description": "The search query"
      },
      "num_results": {
        "type": "integer",
        "description": "Number of search results to return",
        "default": 3,
        "minimum": 1,
        "maximum": 10
      }
    },
    "required": [
      "query"
    ]
  },
  "examples": [
    {
      "query": "stealth product launch announcements",
      "num_results": 5
    },
    {
      "query": "new AI startup launches quietly",
      "num_results": 3
    },
    {
      "query": "cryptocurrency stealth mode launch",
      "num_results": 4
    }
  ]
}